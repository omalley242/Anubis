{
    "language_configs": {
        "rs":{
            "anubis_character": "@",
            "multiline_start": "/*",
            "multiline_end": "*/"
        },
        "md":{
            "anubis_character": "@",
            "multiline_start": "<!--",
            "multiline_end": "-->"
        }
    },
    "anubis_ignore": [
        "./src/**",
        "./.git/**",
        "./target/**",
        "*.anubis",
        "*lock",
        "*toml",
        "*.gitignore"
    ]
}