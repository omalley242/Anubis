{
    "url": "127.0.0.1",
    "language_configs": {
        "rs":{
            "language": "rust",
            "anubis_character": "@",
            "multiline_start": "/*",
            "multiline_end": "*/"
        },
        "md":{
            "language": "markdown",
            "anubis_character": "@",
            "multiline_start": "<!--",
            "multiline_end": "-->"
        },
        "html":{
            "language": "html",
            "anubis_character": "@",
            "multiline_start": "<!--",
            "multiline_end": "-->"
        }
    },
    "anubis_ignore": [
        "./src/**",
        "./.git/**",
        "./target/**",
        "*.anubis",
        "*lock",
        "*toml",
        "*.gitignore",
        "*.db"
    ]
}
